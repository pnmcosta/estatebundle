// Common
homepage          : @(Home)            --> /
posts             : @(Blog)                --> @(/blog/)                 --> homepage
post              : @(Post)                --> @(/blog/{0}/)             --> posts
notices           : @(Notices)             --> @(/notices/)              --> homepage

// Admin area
admin             : @(Admin)               --> @(/admin/*)

// Properties
properties          : @(Properties)       --> @(/properties/*)             --> homepage
detail            : @(Detail)       --> @(/property/{0}/)          --> properties
wishlist          : @(Wishlist)       --> @(/wishlist/*)          --> properties